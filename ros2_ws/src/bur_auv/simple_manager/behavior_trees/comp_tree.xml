 <root BTCPP_format="4" >
    <BehaviorTree ID="MainTree">
        <SequenceWithMemory>
            <RetryUntilSuccessful num_attempts="6">
                <SubTree ID="GateTask"/>
                <SubTree ID="BuoyTask"/>
                <SubTree ID="OctagonTask"/>
            </RetryUntilSuccessful>
        </SequenceWithMemory>
    </BehaviorTree>

    <BehaviorTree ID="GateTask">
        <SequenceWithMemory>
            <Sequence>
                <UpdateStartTarget target="{goal}" />
                <GoToTarget target="{goal}"/>
            </Sequence>
            <Sequence>
                <UpdateGateTarget target="{goal}" />
                <GoToTarget target="{goal}"/>
            </Sequence>
        </SequenceWithMemory>
    </BehaviorTree>

    <BehaviorTree ID="BuoyTask">
        <SequenceWithMemory>
            <Sequence>
                <UpdateBuoyTarget target="{goal}" />
                <GoToTarget target="{goal}"/>
            </Sequence>
            <FireTorpedo>
        </SequenceWithMemory>
    </BehaviorTree>

    <BehaviorTree ID="OctagonTask">
        <SequenceWithMemory>
            <Sequence>
                <UpdateOctagonTarget target="{goal}" />
                <GoToTarget target="{goal}"/>
            </Sequence>
        </SequenceWithMemory>
    </BehaviorTree>
 </root>
