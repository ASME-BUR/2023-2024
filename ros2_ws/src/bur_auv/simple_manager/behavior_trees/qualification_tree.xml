 <root BTCPP_format="4" >
    <BehaviorTree ID="MainTree">
        <SequenceWithMemory>
            <RetryUntilSuccessful num_attempts="6">
                <SubTree ID="GateTask"/>
            </RetryUntilSuccessful>
        </SequenceWithMemory>
    </BehaviorTree>

    <BehaviorTree ID="GateTask">
        <Sequence>
            <GoToGate />
            <GoPastGate />
            <GoBackToStart />
        </Sequence>
    </BehaviorTree>
 </root>
