 <root BTCPP_format="4" >
    <BehaviorTree ID="MainTree">
        <SequenceWithMemory>
            <RetryUntilSuccessful>
                <SubTree ID="GateTask"/>
            </RetryUntilSuccessful>
        </SequenceWithMemory>
    </BehaviorTree>

    <BehaviorTree ID="GateTask">
        <SequenceWithMemory>
            <GoToGate>
            <GoPastGate>
            <GoBackToStart>
        </SequenceWithMemory>
    </BehaviorTree>
 </root>
