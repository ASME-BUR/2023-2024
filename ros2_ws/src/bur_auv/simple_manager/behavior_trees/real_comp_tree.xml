 <root BTCPP_format="4" >
    <BehaviorTree ID="MainTree">
        <SequenceWithMemory>
            <RetryUntilSuccessful num_attempts="6">
                <SubTree ID="GateTask"/>
                <SubTree ID="BuoyTask"/>
                <SubTree ID="OctagonTask"/>
            </RetryUntilSuccessful>
        </SequenceWithMemory>
    </BehaviorTree>

    <BehaviorTree ID="GateTask">
        <SequenceWithMemory>
            <Submerge />
            <GoToGate />
            <BarrelRoll />
            <GoPastGate />
        </SequenceWithMemory>
    </BehaviorTree>

    <BehaviorTree ID="BuoyTask">
        <SequenceWithMemory>
            <TurnTowardsBuoy />
            <DriveAtBuoy />
            <FireTorpedoes />
        </SequenceWithMemory>
    </BehaviorTree>

    <BehaviorTree ID="OctagonTask">
        <SequenceWithMemory>
            <TurnTowardsOctagon />
            <DriveTowardsOctagon />
        </SequenceWithMemory
    </BehaviorTree>
 </root>
