 <root BTCPP_format="4" >
    <BehaviorTree ID="MainTree">
        <Sequence>
            <SubTree ID="GateTask"/>
            <SubTree ID="BuoyTask"/>
            <SubTree ID="OctagonTask"/>
        </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="GateTask">
        <Sequence>
            <Submerge />
            <!-- <SavePose output="{start}"/> -->
            <GoToGate />
            <YawRoll />
            <GoPastGate />
        </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="BuoyTask">
        <Sequence>
            <TurnTowardsBuoy />
            <DriveAtBuoy />
            <FireTorpedoes />
            <DriveIntoBuoy />
        </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="OctagonTask">
        <Sequence>
            <!-- <TurnTowardsOctagon orientation="{start}"/> -->
            <Sequence>
                <TurnOctagonRight />
                <DriveZigzag />
            </Sequence>
            <Sequence>
                <TurnOctagonLeft />
                <DriveZigzag />
            </Sequence>
            <!-- <DriveToOctagon /> -->
        </Sequence>
    </BehaviorTree>
 </root>
