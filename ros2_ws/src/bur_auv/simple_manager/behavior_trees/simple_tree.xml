 <root BTCPP_format="4" >
    <BehaviorTree ID="MainTree">
        <Sequence>
            <SubTree ID="GateTask"/>
            <SubTree ID="TorpedoTask"/>
        </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="GateTask">
        <Sequence>
            <UpdateGateTargets name="update_gate_targets">
            <GoInFrontOfGate name="visit_gate_front"/>
            <GoToTarget name="visit_gate">
        </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="TorpedoTask">
        <Sequence>
            <TorpedoVisited name="torpedo_visited"/>
            <Sequence name="torpedo_sequence">
                <VisitTorpedo name="visit_torpedo"/>
                <FireTorpedo name="fire_torpedo"/>
            </Sequence>
        </Sequence>    
    </BehaviorTree>
 </root>
